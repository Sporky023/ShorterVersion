<h1>Listing summary_votes</h1>

<table>
  <tr>
    <th>Summary</th>
    <th>Score</th>
  </tr>

<% @summary_votes.each do |summary_vote| %>
  <tr>
    <td><%=h summary_vote.summary_id %></td>
    <td><%=h summary_vote.score %></td>
    <td><%= link_to 'Show', summary_vote %></td>
    <td><%= link_to 'Edit', edit_summary_vote_path(summary_vote) %></td>
    <td><%= link_to 'Destroy', summary_vote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New summary_vote', new_summary_vote_path %>