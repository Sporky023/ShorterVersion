<h1>New summary_vote</h1>

<% form_for(@summary_vote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :summary_id %><br />
    <%= f.text_field :summary_id %>
  </p>
  <p>
    <%= f.label :score %><br />
    <%= f.text_field :score %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', summary_votes_path %>